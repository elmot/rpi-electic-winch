/ {
	chosen {
		zephyr,console = &cdc_acm_uart0;
	};
	aliases {
		pwm-motor0 = &pwm_motor0;
	};

	pwm_motors {
		compatible = "pwm-leds";
		status = "okay";
		pwm_motor0: pwm_motor0 {
			pwms = <&pwm 9 PWM_MSEC(20) PWM_POLARITY_NORMAL>;
			label = "PWM_MOTOR0";
		};

	};
};

&pwm {
	status = "okay";
	divider-frac-4 = <15>;
	divider-int-4 = <255>;
};



&zephyr_udc0 {
	cdc_acm_uart0: cdc_acm_uart0 {
		compatible = "zephyr,cdc-acm-uart";
	};
};
